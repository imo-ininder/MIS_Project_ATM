<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:gravity="center_vertical"
    tools:context="com.attmm.admin.projectt.guidance">

    <com.attmm.admin.projectt.view.SquareFrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <android.support.percent.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:id="@+id/blank1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="14%"
                app:layout_heightPercent="10%"/>

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:id="@+id/imageView3"
                android:src="@drawable/blue_rule"
                android:layout_marginEnd="43dp"
                android:layout_toRightOf="@+id/blank1"
                android:layout_below="@+id/blank1"
                android:padding="3dp"
                app:layout_widthPercent="28%"
                app:layout_heightPercent="100%"
                android:onClick="gotoruleteach" />

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="56%"
                app:layout_heightPercent="100%"
                android:padding="3dp"
                android:layout_below="@+id/imageView3"
                android:id="@+id/imageView2"
                android:src="@drawable/blue_step"
                android:onClick="gotostepteach" />

            <View
                android:id="@+id/blank2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="36%"
                app:layout_heightPercent="1%"/>

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_toRightOf="@+id/blank2"
                app:layout_widthPercent="42%"
                app:layout_heightPercent="100%"
                android:padding="3dp"
                android:id="@+id/imageView4"
                android:src="@drawable/blue_guide2" />

            <View
                android:id="@+id/blank3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="49%"
                app:layout_heightPercent="1%"/>

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="26%"
                app:layout_heightPercent="100%"
                android:id="@+id/imageView5"
                android:src="@drawable/blue_cancell"
                android:layout_below="@+id/imageView3"
                android:layout_toRightOf="@+id/blank3"
                android:layout_marginTop="6dp"
                android:padding="3dp"
                android:onClick="gotocancelteach"
                android:nestedScrollingEnabled="false" />

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="26%"
                app:layout_heightPercent="100%"
                android:id="@+id/imageView6"
                android:src="@drawable/blue_nfc"
                android:padding="3dp"
                android:layout_toRightOf="@+id/blank3"
                android:layout_below="@+id/imageView5"
                android:onClick="gotonfcteach" />

            <View
                android:id="@+id/blank4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="75%"
                app:layout_heightPercent="1%"/>

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="22%"
                app:layout_heightPercent="100%"
                android:id="@+id/imageView9"
                android:src="@drawable/blue_atm"
                android:padding="3dp"
                android:layout_toRightOf="@+id/blank4"/>

            <View
                android:id="@+id/blank5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="70%"
                app:layout_heightPercent="20%"/>

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="26%"
                app:layout_heightPercent="100%"
                android:layout_toRightOf="@+id/blank5"
                android:layout_below="@+id/blank5"
                android:padding="3dp"
                android:id="@+id/imageView7"
                android:src="@drawable/blue_giveup"
                android:onClick="gotogiveupteach" />

            <com.attmm.admin.projectt.view.SquareImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="26%"
                app:layout_heightPercent="100%"
                android:padding="3dp"
                android:id="@+id/imageView8"
                android:src="@drawable/blue_report"
                android:layout_below="@+id/imageView7"
                android:layout_toRightOf="@+id/blank5"
                android:onClick="gotoreportteach" />

        </android.support.percent.PercentRelativeLayout>

    </com.attmm.admin.projectt.view.SquareFrameLayout>

</RelativeLayout>
